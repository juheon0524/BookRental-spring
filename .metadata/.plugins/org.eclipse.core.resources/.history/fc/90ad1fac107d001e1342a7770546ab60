<mapper namespace="com.ezen.mybatis.mapperInterface">
  <select id="selectOne" resultType="com.ezen.mybatis.MemberVO" parameterType="int">
    select * from mybatis where id = #{id}
  </select>
  
   <insert id="insertMember" parameterType = "com.ezen.mybatis.MemberVO" >
      insert into mybatis (id, name, phone, address) values (
        #{id}, #{name}, #{phone}, #{address}
      )
  </insert>
  
  <delete id="deleteMember">
  delete from Author where id = #{id}
  </delete>
</mapper>